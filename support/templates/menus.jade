						li: a#MenuCreateBehaviorSeek(title="Add a Seek behavior to selected object", ng-click='lookUpHandler($event)') Seek
				li
					a#MenuPhysics Physics
					ul
						li: a#MenuPhysicsPointConstraint(title="Rotation of connected objects occur around a single pivot point", ng-click='lookUpHandler($event)') Point Constraint
						li: a#MenuPhysicsHingeConstraint(title="Rotation of connected objects occur around a single axis (or hinge)", ng-click='lookUpHandler($event)') Hinge Constraint
						li: a#MenuPhysicsSliderConstraint(title="Connected objects are restricted to only translation along a single axis", ng-click='lookUpHandler($event)') Slider Constraint
						li: a#MenuPhysicsFixedConstraint(title="Restrict local rotation and local translation of connected objects", ng-click='lookUpHandler($event)') Fixed Constraint
				li
					a#MenuLights Lights
					ul
						li: a#MenuCreateLightPoint(title="Create a point light", ng-click='lookUpHandler($event)') Point
						li: a#MenuCreateLightSpot(title="Create a spot light", ng-click='lookUpHandler($event)') Spot
						li: a#MenuCreateLightDirectional(title="Create a directional light", ng-click='lookUpHandler($event)') Directional
				li
					a#MenuParticles Particles
					ul
						li: a#MenuCreateParticlesBasic(title="Create a basic particle emitter", ng-click='lookUpHandler($event)') Basic
						li: a#MenuCreateParticlesSpray(title="Create a spray particle emitter", ng-click='lookUpHandler($event)') Spray
						li: a#MenuCreateParticlesSuspended(title="Create a suspended (ambient dust) particle emitter", ng-click='lookUpHandler($event)') Suspended
						li: a#MenuCreateParticlesAtmospheric(title="Create an atomspheric (precipitation) particle emitter", ng-click='lookUpHandler($event)') Atmospheric
				li
					a#MenuCameras Cameras
					ul
						li: a#MenuCreateCameraPerspective(title="Create a new perspective camera", ng-click='lookUpHandler($event)') Perspective
				li
					a#Terrain Terrain
					ul
						li: a#MenuCreateTerrain(title="Generate a random terrain with rolling hills", ng-click='lookUpHandler($event)') Terrain
						li(ng-class='{"disabled": !worldHasTerrain}')
							a#MenuCreateTerrainGrass(title="Generate grass on an existing terrain", ng-click='lookUpHandler($event)') Grass
						li: a#MenuCreateTerrainOcean(title="Generate an infinite ocean surface", ng-click='lookUpHandler($event)') Ocean	
				li
					a#GUI GUI Elements
					ul
						li: a#MenuCreateGUIDialog(title="Create a basic dialog box that is optionally added to a selected panel", ng-click='lookUpHandler($event)') Dialog
						li: a#MenuCreateGUIButton(title="Create a button that is optionally added to a selected panel", ng-click='lookUpHandler($event)') Button
						li: a#MenuCreateGUILabel(title="Create a label that is optionally added to a selected panel", ng-click='lookUpHandler($event)') Label
