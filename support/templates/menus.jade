		li
			a#MenuAssets Assets
			ul
				li
					a#MenuAssetsSaveAs
						.glyphicon.glyphicon-save
						| Create New Asset
					ul
						li(ng-class='{"disabled": !selection}')
							a#MenuAssetsSaveAsEntity(title="Create a new reusable asset from the selected entity", ng-click='lookUpHandler($event)')
								.glyphicon.glyphicon-book
								| From Sel. Entity
						li(ng-class='{"disabled": !hasMaterial}')
							a#MenuAssetsSaveAsMaterial(title="Create a new reusable asset from the selected entity's material", ng-click='lookUpHandler($event)')
								.glyphicon.glyphicon-tint
								| From Sel. Material
						li(ng-class='{"disabled": !isBehavior}')
							a#MenuAssetsSaveAsBehavior(title="Create a new reusable asset from the selected behavior", ng-click='lookUpHandler($event)')
								.glyphicon.glyphicon-cog
								| From Sel. Behavior
						li: a#MenuAssetsSaveAsFile(title="Creates a new reusable asset via file upload", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-hdd
							| From File
				li(ng-class='{"disabled": !isEntityAsset && !isMaterialAsset}')
					a#MenuAssetsSave
						.glyphicon.glyphicon-floppy-save
