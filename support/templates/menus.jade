mixin menus_head
	block

mixin menus_body

	#smoothmenu1.ddsmoothmenu(ng-controller='MenuController', style="z-index: 100;"): ul
		li
			a#MenuFile File
			ul
				li(ng-class='{"disabled": loggedIn}'): a#MenuLogIn(ng-click='lookUpHandler($event)')
					.glyphicon.glyphicon-log-in
					| Log In
				li: a#MenuSaveNow(ng-click='lookUpHandler($event)')
					.glyphicon.glyphicon-floppy-disk
					| Save Now
				li(ng-class='{"disabled": isExample}')
					a#MenuShareWorld(title="Share a link to this world with your colleagues", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-share
						| Share
				li(ng-class='{"disabled": isExample || !userIsOwner}')
					a#SetThumbnail(title="Take a screenshot and designate it as the public thumbnail", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-picture
						| Set World Thumbnail
				li(ng-class='{"disabled": allowPlayPause && (isExample || !userIsOwner)}')
					a#TestSettings(title="Change the publish settings of this world", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-globe
						| World Settings (ctrl+9)
				li(ng-class='{"disabled": worldIsNotLaunchable && allowPlayPause}')
					a#TestLaunch(title="Load the world as if it were published", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-play
						| Play World (ctrl+0)

		li
			a#MenuEdit Edit
			ul
				li(ng-class='{disabled: !fields.undoAction}')
					a#MenuUndo(ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-share-alt(style='transform: rotateY(180deg);')
						| Undo {{fields.undoAction}} (ctrl-z)
				li(ng-class='{disabled: !fields.redoAction}')
					a#MenuRedo(ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-share-alt
						| Redo {{fields.redoAction}} (ctrl-y)
				li.divider

				li(ng-class='{"disabled": !selection}')
					a#MenuCopy(title="Copy the currently selected object(s) to the internal clipboard", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-copy
						| Copy (ctrl-c)
				li: a#MenuPaste(title="Paste previously copied object(s) from the internal clipboard", ng-click='lookUpHandler($event)')
					.glyphicon.glyphicon-paste
					| Paste (ctrl-v)
				li(ng-class='{"disabled": !selection}')
					a#MenuDuplicate(title="Duplicate currently selected object(s)", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-duplicate
						| Duplicate (ctrl-s)
				li(ng-class='{"disabled": !selection}')
					a#MenuDelete(title="Delete currently selected object(s)", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-trash
						| Delete (del)
				li.divider

				li: a#MenuShare(title="Specify which users are authorized to modify the world", ng-click='lookUpHandler($event)')
					.glyphicon.glyphicon-share
					| Set Permissions
				li
					a#MenuSelect Select
					ul
						li: a#MenuSelectName(title="Select objects by type and name", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-list
							| Select By Name
						li: a#MenuSelectPick(title="Select objects with the mouse (default)", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-screenshot
							| Pick (q)
						li: a#MenuSelectNone(title="Deselect selected object(s)", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-unchecked
							| Select None
						li: a#MenuSelectScene(title="Select the scene node", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-film
							| Select Scene

				li
					a#MenuTransforms Transform
					ul
						li: a#MenuMove(title="Enable the move tool on the currently selected object(s)", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-move
							| Move (w)
						li: a#MenuRotate(title="Enable the rotate tool on the currently selected object(s)", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-repeat
							| Rotate (e)
						li: a#MenuScale(title="Enable the scale tool on the currently selected object(s)", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-fullscreen
							| Scale (r)
						li: a#MenuMulti(title="Enable all transform tools on the currently selected object(s)", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-screenshot
							| Multi (t)
						li.divider

						li: a#MenuLocal(title="Present transform tools with respect to selected object's local transform", ng-click='lookUpHandler($event)')
							| Local Coords
						li: a#MenuWorld(title="Present transform tools with respect to world (global) coordinates", ng-click='lookUpHandler($event)')
							| World Coords
						li: a#MenuObjectCenters(title="Transform the objects in place, or from the gizmo", ng-click='lookUpHandler($event)')
							| Use Object Center	
						li.divider

						li
							a#MenuSnaps Snaps
							ul
								li: a#MenuSnapLarge(title="Transform changes snap in large increments", ng-click='lookUpHandler($event)') Large
								li: a#MenuSnapMedium(title="Transform changes snap in medium increments", ng-click='lookUpHandler($event)') Medium
								li: a#MenuSnapSmall(title="Transform changes snap in small increments", ng-click='lookUpHandler($event)') Small
								li: a#MenuSnapOff(title="Turns off transform snapping", ng-click='lookUpHandler($event)') Off
						li.divider

						li
							a#ResetTransforms(title="Reset selected object(s) back to the default position, rotation, and scale", ng-click='lookUpHandler($event)')
								| Reset Transforms

		li
			a#MenuCamera Camera
			ul
				
				li(ng-class='{"disabled": !selection}')
					a#MenuFocusSelected(title="Focus editor viewport on the currently selected object(s)", ng-click='lookUpHandler($event)')
						.glyphicon.glyphicon-screenshot
						| Focus Selected
				li
					a Camera Sharing
					ul
						li: a#MenuCameraShare(title="Begin broadcasting editor viewport", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-share
							| Share Camera View
						li: a#MenuCameraReceive(title="Begin viewing remote viewport broadcast, if available", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-share
							| Receive Camera Broadcasts
						li: a#MenuCameraReceive(title="Stop viewing remote viewport broadcast", ng-click='lookUpHandler($event)')
							.glyphicon.glyphicon-share
							| Stop Receiving Camera Broadcasts

				li: a#MenuActivateCamera(title="Choose which camera to view in editor viewport", ng-click='lookUpHandler($event)')
					.glyphicon.glyphicon-video
					| Activate Camera
				li
					a#MenuDefaultCameras
						.glyphicon.glyphicon-new-window
						| Default Cameras
					ul
						li: a#MenuCameraOrbit(title="Camera mode that supports rotation around a fixed point", ng-click='lookUpHandler($event)') Orbit
						li: a#MenuCameraFree(title="Camera mode that emulates a first person view", ng-click='lookUpHandler($event)') Free
						li: a#MenuCamera3RDPerson(title="Camera mode that orients itself behind your avatar", ng-click='lookUpHandler($event)') Avatar View
						li: a#MenuCameraNavigate(title="Camera mode best suited to navigate around large regions of space", ng-click='lookUpHandler($event)') Navigate
						li: a#MenuCameraFly(title="Camera mode that moves forward at a set speed", ng-click='lookUpHandler($event)') Fly
						li: a#MenuCameraDeviceOrientation(title="Camera mode that uses mobile device's sensors to control viewport, if available", ng-click='lookUpHandler($event)')
							| DeviceOrientation
						li
							a#MenuDefaultCameras
								.glyphicon.glyphicon-new-window 
								| Orthographic Views
							ul
								li: a#MenuCameraTop(title="Top schematic view", ng-click='lookUpHandler($event)') Top
								li: a#MenuCameraLeft(title="Left schematic view", ng-click='lookUpHandler($event)') Left
								li: a#MenuCameraFront(title="Front schematic view", ng-click='lookUpHandler($event)') Front

		li(ng-class='{"disabled": !selection}')
