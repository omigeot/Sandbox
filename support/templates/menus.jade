							a#MenuAssetsSaveEntity(title="Overwrite selected entity's source asset with current version", ng-click='lookUpHandler($event)')
								.glyphicon.glyphicon-book
								| From Sel. Entity
						li(ng-class='{"disabled": !isMaterialAsset}')
							a#MenuAssetsSaveMaterial(title="Overwrite selected entity material's source asset with current version", ng-click='lookUpHandler($event)')
								.glyphicon.glyphicon-tint
								| From Sel. Material
						li(ng-class='{"disabled": !isEntityAsset || !isBehavior}')
							a#MenuAssetsSaveBehavior(title="Overwrite selected behavior's source asset with current version", ng-click='lookUpHandler($event)')
